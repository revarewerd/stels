<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-jetty run</actionName>
            <displayName>jetty run</displayName>
            <goals>
                <goal>jetty:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-jetty:stop</actionName>
            <displayName>jetty:stop</displayName>
            <goals>
                <goal>jetty:stop</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-jetty:stop jetty:run</actionName>
            <displayName>jetty:stop jetty:run</displayName>
            <goals>
                <goal>jetty:stop</goal>
                <goal>jetty:run</goal>
            </goals>
            <properties>
                <Env.MAVEN_OPTS>-Xmx512m</Env.MAVEN_OPTS>
            </properties>
        </action>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
                
            </goals>
            <properties>
                <exec.args>-Dwayrecall.webappres.monitoring="/media/Data/wayrecall/monitoring/src/main/webapp-monitoring" -classpath %classpath ru.sosgps.wayrecall.initialization.JettyServer 8082</exec.args>
                <exec.executable>java</exec.executable>
                
            </properties>
        </action>
        <action>
            <actionName>run.single.deploy</actionName>
            <goals>
                <goal>jetty:stop</goal>
                <goal>jetty:run</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-runparser</actionName>
            <displayName>runparser</displayName>
            <goals>
                <goal>scala:run</goal>
            </goals>
            <properties>
                <mainClass>ru.sosgps.wayrecall.wialonparser.users.TabSeparatedParser</mainClass>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-SocketToFileRetrans</actionName>
            <displayName>SocketToFileRetrans</displayName>
            <goals>
                <goal>scala:run</goal>
            </goals>
            <properties>
                <mainClass>ru.sosgps.wayrecall.wialonparser.SocketToFileRetrans</mainClass>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-jetty profile</actionName>
            <displayName>jetty profile</displayName>
            <goals>
                <goal>jetty:run</goal>
            </goals>
            <properties>
                <Env.MAVEN_OPTS>-Xmx256m -XX:+UseLinuxPosixThreadCPUClocks -agentpath:/home/nickl/apps/netbeans-7.1/profiler/lib/deployed/jdk16/linux-amd64/libprofilerinterface.so=/home/nickl/apps/netbeans-7.1/profiler/lib,5140</Env.MAVEN_OPTS>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-WrpReader</actionName>
            <displayName>WrpReader</displayName>
            <goals>
                <goal>scala:run</goal>
            </goals>
            <properties>
                <mainClass>ru.sosgps.wayrecall.wialonparser.WrpReader</mainClass>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Dwayrecall.webappres.monitoring="/media/Data/wayrecall/monitoring/src/main/webapp-monitoring" -classpath %classpath ru.sosgps.wayrecall.initialization.JettyServer 8082</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>${profiler.args} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Dwayrecall.webappres.monitoring="/media/Data/wayrecall/monitoring/src/main/webapp-monitoring" -classpath %classpath ru.sosgps.wayrecall.initialization.JettyServer 8082</exec.args>
                <exec.executable>${profiler.java}</exec.executable>
            </properties>
        </action>
    </actions>
